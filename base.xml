<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="github"       alias="origin" fetch="ssh://git@github.com"       revision="morty" />
    <remote name="openembedded" alias="origin" fetch="git://git.openembedded.org" revision="morty"  />
    <remote name="yoctoproject" alias="origin" fetch="git://git.yoctoproject.org" revision="morty"  />

    <default remote="github"
             sync-c="false"
             sync-j="4" />

    <project path="manifests"
             name="aswild/yocto-manifests" />

    <project path="meta-wild-common"
             name="aswild/meta-wild-common" />

    <project path="openembedded-core"
             name="openembedded-core"
             remote="openembedded" />

    <project path="openembedded-core/bitbake"
             name="bitbake"
             remote="openembedded"
             revision="1.32" />

     <project path="meta-openembedded"
              name="meta-openembedded"
              remote="openembedded" />
</manifest>
