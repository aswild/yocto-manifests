<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="github"       alias="origin" fetch=".."                               revision="master" />
    <remote name="oe"           alias="origin" fetch="https://github.com/openembedded"  revision="langdale" />
    <remote name="yocto"        alias="origin" fetch="https://git.yoctoproject.org/git" revision="langdale" />

    <default remote="github" sync-c="false" sync-j="4" />

    <project path="manifests"                   name="aswild/yocto-manifests" />
    <project path="meta-wild-common"            name="aswild/meta-wild-common" />
    <project path="meta-rust-bin"               name="rust-embedded/meta-rust-bin" />

    <project path="openembedded-core"           name="openembedded-core"    remote="oe" />
    <project path="openembedded-core/bitbake"   name="bitbake"              remote="oe"     revision="2.2" />
    <project path="meta-openembedded"           name="meta-openembedded"    remote="oe" />
</manifest>
