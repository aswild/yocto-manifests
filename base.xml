<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="github"       alias="origin" fetch=".."                            revision="master" />
    <remote name="oe"           alias="origin" fetch="git://github.com/openembedded" revision="sumo"  />
    <remote name="yocto"        alias="origin" fetch="git://git.yoctoproject.org"    revision="sumo"  />

    <default remote="github" sync-c="false" sync-j="4" />

    <project path="manifests"                   name="aswild/yocto-manifests" />
    <project path="meta-wild-common"            name="aswild/meta-wild-common" />

    <project path="openembedded-core"           name="openembedded-core"    remote="oe" />
    <project path="openembedded-core/bitbake"   name="bitbake"              remote="oe"     revision="1.38" />
    <project path="meta-openembedded"           name="meta-openembedded"    remote="oe" />

    <project path="meta-java"                   name="meta-java"            remote="yocto"  revision="master" />
</manifest>
